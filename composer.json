{
  "name":             "northwell-health/plugin-faker",
  "description":      "Faker Support for Pattern Lab",
  "keywords":         ["faker", "pattern lab"],
  "homepage":         "http://patternlab.io",
  "license":          "MIT",
  "type":             "patternlab-plugin",
  "authors": [
    {
      "name":         "Don McGrath",
      "email":        "dmcgrath1@northwell.edu",
      "homepage":     "northwell.edu",
      "role":         "Senior Developer"
    }
  ],
  "support": {
    "issues":         "https://github.com/pattern-lab/plugin-php-faker/issues",
    "wiki":           "http://patternlab.io/docs/",
    "source":         "https://github.com/pattern-lab/plugin-php-faker/releases"
  },
  "autoload": {
    "psr-0": {
      "PatternLab\\Faker": "src/"
    }
  },
  "require": {
    "php":              ">=5.4",
    "pattern-lab/core": "^2.3.0",
    "fzaninotto/faker": "^1.0.0"
  },
  "extra": {
    "patternlab": {
      "config": {
        "plugins": {
          "faker": {
            "enabled": true,
            "locale": "en_US"
          }
        }
      }
    }
  }
}
